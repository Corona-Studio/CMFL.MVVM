<Page x:Class="CMFL.MVVM.Views.GamePage"
      Foreground="{DynamicResource SystemGray6}"
      TextElement.FontWeight="Regular"
      TextElement.FontSize="13"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:converter="clr-namespace:CMFL.MVVM.Class.Converter"
      xmlns:b="http://schemas.microsoft.com/xaml/behaviors"
      DataContext="{Binding GamePageViewModel,Source={StaticResource Locator}}"
      mc:Ignorable="d"
      d:DesignHeight="518" d:DesignWidth="730" AllowDrop="True">
    <Page.Resources>
        <converter:InverseBooleanConverter x:Key="InverseBooleanConverter" />
        <converter:ImageOptimizeConverter x:Key="ImageOptimizeConverter" />
        <Storyboard x:Key="GridBlurRaise">
            <DoubleAnimation
                From="0" To="20"
                SpeedRatio="4.0"
                Storyboard.TargetName="BlurEffect"
                Storyboard.TargetProperty="Radius" />
        </Storyboard>
        <Storyboard x:Key="GridBlurDrop">
            <DoubleAnimation
                From="20" To="0"
                SpeedRatio="4.0"
                Storyboard.TargetName="BlurEffect"
                Storyboard.TargetProperty="Radius" />
        </Storyboard>
        <Storyboard x:Key="GameDownloadGridOpacityRaise">
            <DoubleAnimation
                From="0.00" To="1.00"
                SpeedRatio="1.5"
                Storyboard.TargetName="GridGameDownloadList"
                Storyboard.TargetProperty="Opacity" />
        </Storyboard>
        <Storyboard x:Key="GameDownloadGridOpacityDrop">
            <DoubleAnimation
                From="1.00" To="0.00"
                SpeedRatio="1.5"
                Storyboard.TargetName="GridGameDownloadList"
                Storyboard.TargetProperty="Opacity" />
        </Storyboard>
        <Storyboard x:Key="GamePathGridOpacityRaise">
            <DoubleAnimation
                From="0.00" To="1.00"
                SpeedRatio="1.5"
                Storyboard.TargetName="GamePathGrid"
                Storyboard.TargetProperty="Opacity" />
        </Storyboard>
        <Storyboard x:Key="GamePathGridOpacityDrop">
            <DoubleAnimation
                From="1.00" To="0.00"
                SpeedRatio="1.5"
                Storyboard.TargetName="GamePathGrid"
                Storyboard.TargetProperty="Opacity" />
        </Storyboard>
        <Storyboard x:Key="GridDragCheckOpacityRaise">
            <DoubleAnimation
                From="0.00" To="1.00"
                SpeedRatio="1.5"
                Storyboard.TargetName="DragCheckGrid"
                Storyboard.TargetProperty="Opacity" />
        </Storyboard>
        <Storyboard x:Key="GridDragCheckOpacityDrop">
            <DoubleAnimation
                From="1.00" To="0.00"
                SpeedRatio="1.5"
                Storyboard.TargetName="DragCheckGrid"
                Storyboard.TargetProperty="Opacity" />
        </Storyboard>
        <Style TargetType="ListViewItem">
            <Setter Property="Background" Value="Transparent" />
        </Style>
    </Page.Resources>
    <Grid>
        <Grid>
            <Grid.Effect>
                <BlurEffect x:Name="BlurEffect" Radius="0" />
            </Grid.Effect>
            <Grid.RowDefinitions>
                <RowDefinition Height="10*" />
                <RowDefinition Height="40*" />
                <RowDefinition Height="55*" />
                <RowDefinition Height="533*" />
            </Grid.RowDefinitions>
            <Grid Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="441*" />
                    <ColumnDefinition Width="176*" />
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0" Background="{StaticResource SystemOpacityWhite3}" Margin="10,0,0,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="31*" />
                        <ColumnDefinition Width="299*" />
                    </Grid.ColumnDefinitions>
                    <materialDesign:PackIcon Margin="5" Grid.Column="0" Kind="Information" Padding="5"
                                             HorizontalAlignment="Stretch"
                                             VerticalAlignment="Stretch" Foreground="{DynamicResource SystemBlue2}"
                                             Width="Auto"
                                             Height="Auto" />
                    <TextBlock Grid.Column="1" Padding="2.5" VerticalAlignment="Center" HorizontalAlignment="Center"
                               Foreground="{DynamicResource SystemGray6}" Text="{DynamicResource DragSupport}" />
                </Grid>
            </Grid>
            <Grid Grid.Row="2">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="64*" />
                    <ColumnDefinition Width="247*" />
                    <ColumnDefinition Width="135*" />
                    <ColumnDefinition Width="171*" />
                </Grid.ColumnDefinitions>
                <materialDesign:PackIcon Kind="Server" Foreground="{DynamicResource SystemGreen1}" Grid.Column="0"
                                         Height="25" Width="25"
                                         VerticalAlignment="Stretch" HorizontalAlignment="Stretch" />
                <TextBlock Text="{DynamicResource GameList}" FontSize="25" Foreground="{DynamicResource SystemGray6}"
                           Grid.Column="1"
                           HorizontalAlignment="Left" VerticalAlignment="Center" />
                <Button Grid.Column="2" Background="{x:Null}" BorderBrush="{x:Null}"
                        Style="{DynamicResource MaterialDesignRaisedButton}"
                        Command="{Binding OpenGamePathGridCommand}" Height="Auto" VerticalAlignment="Center">
                    <Button.Triggers>
                        <EventTrigger RoutedEvent="Button.Click">
                            <BeginStoryboard Storyboard="{StaticResource GridBlurRaise}" />
                            <BeginStoryboard Storyboard="{StaticResource GamePathGridOpacityRaise}" />
                        </EventTrigger>
                    </Button.Triggers>
                    <StackPanel Orientation="Horizontal">
                        <materialDesign:PackIcon Kind="FolderStar" Width="25" Height="25"
                                                 Foreground="{DynamicResource SystemGray6}"
                                                 VerticalAlignment="Center" HorizontalAlignment="Center" />
                        <TextBlock Text="{DynamicResource GamePath}" Foreground="{DynamicResource SystemGray6}"
                                   FontWeight="Normal"
                                   FontSize="16" VerticalAlignment="Center"
                                   HorizontalAlignment="Center" />
                    </StackPanel>
                </Button>
                <Button Grid.Column="3" Background="{x:Null}" BorderBrush="{x:Null}"
                        Style="{DynamicResource MaterialDesignRaisedButton}"
                        Command="{Binding OpenGameDownloadGridCommand}" VerticalAlignment="Center" Height="Auto">
                    <Button.Triggers>
                        <EventTrigger RoutedEvent="Button.Click">
                            <BeginStoryboard Storyboard="{StaticResource GridBlurRaise}" />
                            <BeginStoryboard Storyboard="{StaticResource GameDownloadGridOpacityRaise}" />
                        </EventTrigger>
                    </Button.Triggers>
                    <StackPanel Orientation="Horizontal">
                        <materialDesign:PackIcon Kind="BriefcaseDownload" Width="25" Height="25"
                                                 Foreground="{DynamicResource SystemGray6}"
                                                 VerticalAlignment="Center" HorizontalAlignment="Center" />
                        <TextBlock Text="{DynamicResource GetGames}" Foreground="{DynamicResource SystemGray6}"
                                   FontWeight="Normal"
                                   FontSize="16" VerticalAlignment="Center"
                                   HorizontalAlignment="Center" />
                    </StackPanel>
                </Button>
            </Grid>
            <Grid Grid.Row="3">
                <Grid.RowDefinitions>
                    <RowDefinition Height="8*" />
                    <RowDefinition Height="115*" />
                </Grid.RowDefinitions>
                <Button Grid.Row="0" Background="{StaticResource SystemOpacityWhite3}"
                        BorderBrush="{StaticResource SystemOpacityWhite3}"
                        Command="{Binding RefreshGameListCommand}" HorizontalAlignment="Left" Height="Auto"
                        Margin="20,0,0,0">
                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
                        <materialDesign:PackIcon Kind="Autorenew" Width="18" Height="18" />
                        <TextBlock Text="{Binding RefreshButtonText}" HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   FontSize="15" />
                    </StackPanel>
                </Button>
                <Grid Grid.Row="1">
                    <Border CornerRadius="5"
                            Visibility="{Binding EmptyGameListGridVisibility, Converter={StaticResource BooleanToVisibilityConverter}}"
                            Background="{StaticResource SystemOpacityWhite3}" VerticalAlignment="Center"
                            HorizontalAlignment="Center">
                        <TextBlock Padding="10" VerticalAlignment="Center" HorizontalAlignment="Center">
                            <InlineUIContainer>
                                <StackPanel Orientation="Horizontal">
                                    <materialDesign:PackIcon Kind="Dropbox" VerticalAlignment="Center"
                                                             HorizontalAlignment="Center"
                                                             Width="60" Height="60"
                                                             Foreground="{DynamicResource SystemBrown1}" />
                                    <StackPanel VerticalAlignment="Center">
                                        <TextBlock Foreground="{DynamicResource SystemGray6}" FontSize="24"
                                                   Text="{DynamicResource EmptyGameList}" />
                                        <TextBlock Foreground="{DynamicResource SystemGray3}"
                                                   Text="{DynamicResource EmptyGameListDetail}" />
                                    </StackPanel>
                                </StackPanel>
                            </InlineUIContainer>
                        </TextBlock>
                    </Border>
                    <ScrollViewer VerticalScrollBarVisibility="Auto" PreviewMouseWheel="UIElement_OnPreviewMouseWheel">
                        <ListView BorderBrush="{x:Null}" Foreground="{x:Null}"
                                  VirtualizingStackPanel.IsVirtualizing="True"
                                  VirtualizingStackPanel.VirtualizationMode="Recycling"
                                  ItemsSource="{Binding GameInfo, Mode=OneWay}"
                                  Background="{x:Null}" HorizontalContentAlignment="Center">
                            <ListView.ItemTemplate>
                                <DataTemplate>
                                    <Grid MinWidth="600">
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="10*" />
                                            <RowDefinition Height="49*" />
                                            <RowDefinition Height="26*" />
                                        </Grid.RowDefinitions>
                                        <Grid.Triggers>
                                            <EventTrigger RoutedEvent="Mouse.MouseEnter">
                                                <BeginStoryboard>
                                                    <Storyboard Storyboard.TargetName="ContentPanel"
                                                                Storyboard.TargetProperty="Margin">
                                                        <ThicknessAnimationUsingKeyFrames>
                                                            <EasingThicknessKeyFrame KeyTime="0" Value="0">
                                                                <EasingThicknessKeyFrame.EasingFunction>
                                                                    <PowerEase EasingMode="EaseOut" Power="8" />
                                                                </EasingThicknessKeyFrame.EasingFunction>
                                                            </EasingThicknessKeyFrame>
                                                            <EasingThicknessKeyFrame KeyTime="0:0:0.4" Value="10,0,0,0">
                                                                <EasingThicknessKeyFrame.EasingFunction>
                                                                    <PowerEase EasingMode="EaseOut" Power="8" />
                                                                </EasingThicknessKeyFrame.EasingFunction>
                                                            </EasingThicknessKeyFrame>
                                                        </ThicknessAnimationUsingKeyFrames>
                                                    </Storyboard>
                                                </BeginStoryboard>
                                            </EventTrigger>
                                            <EventTrigger RoutedEvent="Mouse.MouseLeave">
                                                <BeginStoryboard>
                                                    <Storyboard>
                                                        <ThicknessAnimationUsingKeyFrames
                                                            Storyboard.TargetName="ContentPanel"
                                                            Storyboard.TargetProperty="Margin">
                                                            <EasingThicknessKeyFrame KeyTime="0" Value="10,0,0,0">
                                                                <EasingThicknessKeyFrame.EasingFunction>
                                                                    <PowerEase EasingMode="EaseOut" Power="8" />
                                                                </EasingThicknessKeyFrame.EasingFunction>
                                                            </EasingThicknessKeyFrame>
                                                            <EasingThicknessKeyFrame KeyTime="0:0:0.8" Value="0">
                                                                <EasingThicknessKeyFrame.EasingFunction>
                                                                    <PowerEase EasingMode="EaseOut" Power="8" />
                                                                </EasingThicknessKeyFrame.EasingFunction>
                                                            </EasingThicknessKeyFrame>
                                                        </ThicknessAnimationUsingKeyFrames>
                                                    </Storyboard>
                                                </BeginStoryboard>
                                            </EventTrigger>
                                        </Grid.Triggers>
                                        <Grid Grid.Row="1" Background="{StaticResource SystemOpacityWhite1}">
                                            <RadioButton GroupName="GameVersion"
                                                         MinHeight="80"
                                                         Height="80"
                                                         Style="{StaticResource RadioButtonStyle1}"
                                                         Background="{StaticResource SystemOpacityWhite3}"
                                                         Command="{Binding ChooseGameCommand}"
                                                         IsChecked="{Binding IsChecked}">
                                                <Grid Width="600">
                                                    <StackPanel x:Name="ContentPanel" Orientation="Horizontal"
                                                                HorizontalAlignment="Left"
                                                                Margin="0,0,0,0"
                                                                ScrollViewer.VerticalScrollBarVisibility="Auto"
                                                                Width="Auto" Grid.ColumnSpan="1" MaxWidth="600">
                                                        <Image Width="35" Height="35"
                                                               Source="{Binding Icon, Converter={StaticResource ImageOptimizeConverter}}" />
                                                        <TextBlock Text="{Binding Name}"
                                                                   ToolTip="{Binding Name}"
                                                                   Foreground="{DynamicResource PrimaryLabelColor}"
                                                                   MaxWidth="300" TextTrimming="CharacterEllipsis"
                                                                   HorizontalAlignment="Center" FontSize="15"
                                                                   VerticalAlignment="Center" Background="{x:Null}"
                                                                   Margin="5,0,0,0" />
                                                        <StackPanel Orientation="Horizontal"
                                                                    HorizontalAlignment="Center"
                                                                    VerticalAlignment="Center" Margin="5,0,0,0">
                                                            <Border CornerRadius="3" Padding="2.5" Margin="3"
                                                                    Background="{DynamicResource SystemYellow1}"
                                                                    VerticalAlignment="Center"
                                                                    HorizontalAlignment="Center">
                                                                <TextBlock Text="{Binding Version}" MaxWidth="100"
                                                                           ToolTip="{Binding Version}"
                                                                           Foreground="{DynamicResource PrimaryLabelColor}"
                                                                           TextTrimming="CharacterEllipsis"
                                                                           FontSize="10" />
                                                            </Border>
                                                            <Border CornerRadius="3" Padding="2.5"
                                                                    Background="{DynamicResource SystemIndigo1}"
                                                                    Margin="3">
                                                                <TextBlock Text="{DynamicResource Local}" FontSize="10" />
                                                            </Border>
                                                        </StackPanel>
                                                    </StackPanel>
                                                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Right"
                                                                VerticalAlignment="Center" Margin="0,0,30,0">
                                                        <Button
                                                            Command="{x:Static materialDesign:DialogHost.OpenDialogCommand}"
                                                            Style="{StaticResource MaterialDesignFloatingActionMiniDarkButton}"
                                                            Background="{x:Null}" BorderBrush="{x:Null}">
                                                            <Button.CommandParameter>
                                                                <StackPanel Width="530">
                                                                    <TextBlock Padding="10" HorizontalAlignment="Left"
                                                                               FontSize="18">
                                                                        <Run Text="{Binding Name}" />
                                                                        <Run Text="{DynamicResource Settings}" />
                                                                    </TextBlock>
                                                                    <ScrollViewer Height="300">
                                                                        <StackPanel>
                                                                            <Grid Margin="30,0">
                                                                                <Grid.ColumnDefinitions>
                                                                                    <ColumnDefinition Width="37*" />
                                                                                    <ColumnDefinition Width="159*" />
                                                                                    <ColumnDefinition Width="109*" />
                                                                                    <ColumnDefinition Width="32*" />
                                                                                    <ColumnDefinition Width="109*" />
                                                                                    <ColumnDefinition Width="32*" />
                                                                                </Grid.ColumnDefinitions>
                                                                                <Grid.ToolTip>
                                                                                    <StackPanel
                                                                                        Orientation="Horizontal">
                                                                                        <materialDesign:PackIcon
                                                                                            Kind="Alert"
                                                                                            Foreground="{DynamicResource SystemYellow1}" />
                                                                                        <TextBlock Padding="5,0"
                                                                                                   Text="{DynamicResource MemorySettingTip}" />
                                                                                    </StackPanel>
                                                                                </Grid.ToolTip>
                                                                                <materialDesign:PackIcon Kind="Memory"
                                                                                                         Foreground="{DynamicResource SystemBlue1}"
                                                                                                         Width="20"
                                                                                                         Height="20"
                                                                                                         HorizontalAlignment="Center"
                                                                                                         VerticalAlignment="Center"
                                                                                                         Grid.Column="0" />
                                                                                <TextBlock
                                                                                    Text="{DynamicResource LaunchMemory}"
                                                                                    Padding="10"
                                                                                    FontSize="12" Grid.Column="1"
                                                                                    VerticalAlignment="Center"
                                                                                    HorizontalAlignment="Left" />
                                                                                <TextBox
                                                                                    Text="{Binding GameSettings.MinMemory, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                                    Grid.Column="2"
                                                                                    VerticalAlignment="Center"
                                                                                    materialDesign:HintAssist.Hint="{DynamicResource MinMem}" />
                                                                                <TextBlock Text="MB"
                                                                                           HorizontalAlignment="Center"
                                                                                           Grid.Column="3"
                                                                                           VerticalAlignment="Center" />
                                                                                <TextBox
                                                                                    Text="{Binding GameSettings.MaxMemory, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                                    Grid.Column="4"
                                                                                    VerticalAlignment="Center"
                                                                                    materialDesign:HintAssist.Hint="{DynamicResource LaunchMemory}" />
                                                                                <TextBlock Text="MB"
                                                                                           HorizontalAlignment="Center"
                                                                                           Grid.Column="5"
                                                                                           VerticalAlignment="Center" />
                                                                            </Grid>
                                                                            <Grid Margin="30,0">
                                                                                <Grid.ColumnDefinitions>
                                                                                    <ColumnDefinition Width="37*" />
                                                                                    <ColumnDefinition Width="159*" />
                                                                                    <ColumnDefinition Width="283*" />
                                                                                </Grid.ColumnDefinitions>
                                                                                <materialDesign:PackIcon
                                                                                    Kind="ServerNetwork"
                                                                                    Foreground="{DynamicResource SystemGreen1}"
                                                                                    Width="20" Height="20"
                                                                                    HorizontalAlignment="Center"
                                                                                    VerticalAlignment="Center"
                                                                                    Grid.Column="0" />
                                                                                <TextBlock Grid.Column="1" Padding="10"
                                                                                           Text="{DynamicResource JoinServerAfterLaunch}"
                                                                                           FontSize="12" />
                                                                                <TextBox
                                                                                    Grid.Column="2"
                                                                                    Text="{Binding GameSettings.ServerIp, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                                    VerticalAlignment="Center">
                                                                                    <materialDesign:HintAssist.Hint>
                                                                                        <Run
                                                                                            Text="{DynamicResource ServerIP}" />
                                                                                    </materialDesign:HintAssist.Hint>
                                                                                </TextBox>
                                                                            </Grid>
                                                                            <materialDesign:Card
                                                                                HorizontalAlignment="Stretch"
                                                                                Background="{DynamicResource SystemGray2}"
                                                                                Margin="5,0">
                                                                                <StackPanel>
                                                                                    <Expander
                                                                                        Width="480"
                                                                                        Background="{x:Null}">
                                                                                        <Expander.Header>
                                                                                            <StackPanel
                                                                                                Orientation="Horizontal"
                                                                                                HorizontalAlignment="Left">
                                                                                                <materialDesign:PackIcon
                                                                                                    Kind="DecagramOutline"
                                                                                                    Width="20"
                                                                                                    Height="20"
                                                                                                    VerticalAlignment="Center"
                                                                                                    HorizontalAlignment="Center"
                                                                                                    Foreground="#DDFF0000" />
                                                                                                <TextBlock Padding="5"
                                                                                                           FontSize="12"
                                                                                                           HorizontalAlignment="Center"
                                                                                                           VerticalAlignment="Center"
                                                                                                           Text="{DynamicResource AdvanceLaunchSetting}" />
                                                                                            </StackPanel>
                                                                                        </Expander.Header>
                                                                                        <StackPanel
                                                                                            Orientation="Vertical"
                                                                                            TextBlock.Foreground="{DynamicResource MaterialDesignBody}"
                                                                                            HorizontalAlignment="Center"
                                                                                            VerticalAlignment="Center">
                                                                                            <StackPanel
                                                                                                Orientation="Horizontal"
                                                                                                Margin="10,0">
                                                                                                <materialDesign:PackIcon
                                                                                                    Kind="AlertDecagram"
                                                                                                    Foreground="#ef5350"
                                                                                                    Width="15"
                                                                                                    Height="15"
                                                                                                    VerticalAlignment="Center" />
                                                                                                <TextBlock
                                                                                                    Text="{DynamicResource AdvanceLaunchWarning}"
                                                                                                    Padding="5" />
                                                                                            </StackPanel>
                                                                                            <StackPanel Margin="10,10">
                                                                                                <materialDesign:Card
                                                                                                    Background="#33FFFFFF">
                                                                                                    <StackPanel
                                                                                                        Margin="10,10">
                                                                                                        <Grid>
                                                                                                            <TextBlock
                                                                                                                Padding="5"
                                                                                                                Text="{DynamicResource EnableGc}"
                                                                                                                HorizontalAlignment="Left" />
                                                                                                            <ToggleButton
                                                                                                                x:Name="EnableGcToggle"
                                                                                                                IsChecked="{Binding GameSettings.EnableGc, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                                                                Background="#FF64DD17"
                                                                                                                HorizontalAlignment="Right" />
                                                                                                        </Grid>
                                                                                                        <Grid
                                                                                                            Margin="0,5,0,0"
                                                                                                            IsEnabled="{Binding IsChecked, ElementName=EnableGcToggle}">
                                                                                                            <TextBlock
                                                                                                                Text="{DynamicResource GcType}"
                                                                                                                Padding="5"
                                                                                                                HorizontalAlignment="Left" />
                                                                                                            <ComboBox
                                                                                                                SelectedIndex="{Binding SelectedGcTypeIndex}"
                                                                                                                HorizontalAlignment="Right"
                                                                                                                Width="100"
                                                                                                                materialDesign:HintAssist.Hint="{DynamicResource GcType}">
                                                                                                                <ComboBoxItem
                                                                                                                    Content="{DynamicResource None}" />
                                                                                                                <ComboBoxItem>G1GC</ComboBoxItem>
                                                                                                                <ComboBoxItem>SerialGc</ComboBoxItem>
                                                                                                                <ComboBoxItem>ParallelGc</ComboBoxItem>
                                                                                                                <ComboBoxItem>CmsGc</ComboBoxItem>
                                                                                                            </ComboBox>
                                                                                                        </Grid>
                                                                                                        <Grid
                                                                                                            HorizontalAlignment="Center">
                                                                                                            <Grid.ColumnDefinitions>
                                                                                                                <ColumnDefinition
                                                                                                                    Width="17*" />
                                                                                                                <ColumnDefinition
                                                                                                                    Width="347*" />
                                                                                                            </Grid.ColumnDefinitions>
                                                                                                            <materialDesign:PackIcon
                                                                                                                Kind="CodeArray"
                                                                                                                Foreground="#ff5252"
                                                                                                                Width="15"
                                                                                                                Height="15" />
                                                                                                            <TextBlock
                                                                                                                Foreground="#DD000000"
                                                                                                                Grid.Column="1">
                                                                                                                <Run
                                                                                                                    Text="{DynamicResource AddParameterManually}" />
                                                                                                                <Run
                                                                                                                    Foreground="#ff5252"
                                                                                                                    Text="{DynamicResource AddParameterManuallyHighlight}" />
                                                                                                            </TextBlock>
                                                                                                        </Grid>
                                                                                                        <TextBox
                                                                                                            HorizontalAlignment="Center"
                                                                                                            Foreground="#DD000000"
                                                                                                            Width="418"
                                                                                                            Text="{Binding GameSettings.AdvancedArguments, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                                                                                            <materialDesign:HintAssist.Hint>
                                                                                                                <Run
                                                                                                                    Foreground="#DD000000"
                                                                                                                    Text="{DynamicResource ParameterInputHint}" />
                                                                                                            </materialDesign:HintAssist.Hint>
                                                                                                        </TextBox>
                                                                                                    </StackPanel>
                                                                                                </materialDesign:Card>
                                                                                            </StackPanel>
                                                                                        </StackPanel>
                                                                                    </Expander>
                                                                                </StackPanel>
                                                                            </materialDesign:Card>
                                                                            <Grid Margin="30,0">
                                                                                <Grid.ColumnDefinitions>
                                                                                    <ColumnDefinition Width="35*" />
                                                                                    <ColumnDefinition Width="105*" />
                                                                                    <ColumnDefinition Width="113*" />
                                                                                    <ColumnDefinition Width="20*" />
                                                                                    <ColumnDefinition Width="112*" />
                                                                                    <ColumnDefinition Width="94*" />
                                                                                </Grid.ColumnDefinitions>
                                                                                <materialDesign:PackIcon
                                                                                    Kind="Fullscreen"
                                                                                    HorizontalAlignment="Center"
                                                                                    VerticalAlignment="Center"
                                                                                    Width="20"
                                                                                    Height="20" />
                                                                                <TextBlock
                                                                                    Text="{DynamicResource ScreenResolution}"
                                                                                    Padding="10"
                                                                                    FontSize="12" Grid.Column="1"
                                                                                    VerticalAlignment="Center"
                                                                                    HorizontalAlignment="Left" />
                                                                                <TextBox
                                                                                    Text="{Binding GameSettings.ScreenSize.Width, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                                    Grid.Column="2"
                                                                                    VerticalAlignment="Center"
                                                                                    materialDesign:HintAssist.Hint="{DynamicResource Width}" />
                                                                                <TextBlock Text="*" Grid.Column="3"
                                                                                           HorizontalAlignment="Center"
                                                                                           VerticalAlignment="Center" />
                                                                                <TextBox
                                                                                    Text="{Binding GameSettings.ScreenSize.Height, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                                    Grid.Column="4"
                                                                                    VerticalAlignment="Center"
                                                                                    materialDesign:HintAssist.Hint="{DynamicResource Height}" />
                                                                                <CheckBox Background="#FF3AA0B7"
                                                                                          Content="{DynamicResource FullScreen}"
                                                                                          Grid.Column="5"
                                                                                          IsChecked="{Binding GameSettings.ScreenSize.FullScreen, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                                          HorizontalAlignment="Right" />
                                                                            </Grid>
                                                                            <Grid Margin="30,0">
                                                                                <Grid.ColumnDefinitions>
                                                                                    <ColumnDefinition Width="37*" />
                                                                                    <ColumnDefinition Width="159*" />
                                                                                    <ColumnDefinition Width="283*" />
                                                                                </Grid.ColumnDefinitions>
                                                                                <Grid.ToolTip>
                                                                                    <StackPanel
                                                                                        Orientation="Horizontal">
                                                                                        <materialDesign:PackIcon
                                                                                            Kind="Alert"
                                                                                            Foreground="#FFFFAE00" />
                                                                                        <TextBlock Padding="5,0,0,0"
                                                                                                   Text="{DynamicResource JavaPathToolTip}" />
                                                                                        <TextBlock
                                                                                            Foreground="#FFFFAE00"
                                                                                            Text="{DynamicResource JavaPathToolTipHighlight}" />
                                                                                    </StackPanel>
                                                                                </Grid.ToolTip>
                                                                                <materialDesign:PackIcon
                                                                                    Kind="LanguageJava"
                                                                                    Foreground="#DDFB7E00"
                                                                                    Width="20" Height="20"
                                                                                    HorizontalAlignment="Center"
                                                                                    VerticalAlignment="Center" />
                                                                                <TextBlock
                                                                                    Text="{DynamicResource JavaPath}"
                                                                                    FontSize="12"
                                                                                    Grid.Column="1"
                                                                                    VerticalAlignment="Center"
                                                                                    Padding="10"
                                                                                    HorizontalAlignment="Left" />
                                                                                <TextBox
                                                                                    Text="{Binding GameSettings.JavaPath, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                                                                    BorderBrush="#B2000000"
                                                                                    Grid.Column="2"
                                                                                    materialDesign:HintAssist.Hint="{DynamicResource SelectJavaHint}" />
                                                                            </Grid>
                                                                            <Grid HorizontalAlignment="Right"
                                                                                  Margin="0,0,30,0">
                                                                                <Button
                                                                                    Style="{DynamicResource MaterialDesignRaisedButton}"
                                                                                    Content="{DynamicResource Browse}"
                                                                                    BorderBrush="{x:Null}"
                                                                                    Foreground="#3949ab"
                                                                                    Background="{x:Null}"
                                                                                    Command="{Binding BrowseJavaCommand}" />
                                                                            </Grid>
                                                                            <Grid Margin="30,0">
                                                                                <Grid.ColumnDefinitions>
                                                                                    <ColumnDefinition Width="37*" />
                                                                                    <ColumnDefinition Width="194*" />
                                                                                    <ColumnDefinition Width="247*" />
                                                                                </Grid.ColumnDefinitions>
                                                                                <Grid.ToolTip>
                                                                                    <StackPanel
                                                                                        Orientation="Horizontal">
                                                                                        <materialDesign:PackIcon
                                                                                            Kind="Alert"
                                                                                            Foreground="#FFFFAE00" />
                                                                                        <TextBlock Padding="5,0,0,0"
