                        <TabItem.Header>
                            <StackPanel Width="65" Background="#02000000">
                                <materialDesign:PackIcon Kind="Cube" Width="25" Height="25"
                                                         HorizontalAlignment="Center"
                                                         Foreground="{DynamicResource SystemPink1}" />
                                <TextBlock Foreground="{DynamicResource SystemGray6}" Text="Liteloader"
                                           HorizontalAlignment="Center"
                                           Padding="5" />
                            </StackPanel>
                        </TabItem.Header>
                        <Grid>
                            <ListView
                                    VerticalAlignment="Top"
                                    BorderBrush="{x:Null}" Foreground="{x:Null}"
                                    HorizontalContentAlignment="Left"
                                    IsSynchronizedWithCurrentItem="True"
                                    Background="{x:Null}"
                                    SelectionMode="Single" ItemsSource="{Binding LiteLoaderBuilds, Mode=OneWay}"
                                    ScrollViewer.VerticalScrollBarVisibility="Visible"
                                    ScrollViewer.HorizontalScrollBarVisibility="Hidden"
                                    VirtualizingStackPanel.IsVirtualizing="True"
                                    VirtualizingStackPanel.VirtualizationMode="Recycling" Height="390">
                                <ListView.ItemTemplate>
                                    <DataTemplate>
                                        <StackPanel>
                                            <Grid Width="560" Background="Transparent" Margin="0,0,0,0">
                                                <StackPanel Margin="5,0,0,0">
                                                    <TextBlock TextWrapping="Wrap" Foreground="White" Padding="2.5"
                                                                   HorizontalAlignment="Left" FontSize="20"
                                                                   Text="{Binding Version}" VerticalAlignment="Center" />
                                                    <StackPanel Orientation="Horizontal">
                                                        <Border
                                                                materialDesign:ShadowAssist.ShadowDepth="Depth0"
                                                                CornerRadius="3" HorizontalAlignment="Left"
                                                                Margin="5,0,0,0" Padding="5,2.5"
                                                                Background="{DynamicResource SystemOrange1}">
                                                            <TextBlock Text="{Binding McVersion}"
                                                                           Foreground="White" />
                                                        </Border>
                                                        <Border
                                                                materialDesign:ShadowAssist.ShadowDepth="Depth0"
                                                                CornerRadius="3" HorizontalAlignment="Left"
                                                                Margin="5,0,0,0" Padding="5,2.5"
                                                                Background="{Binding VersionTypeBgColor}">
                                                            <TextBlock Text="{Binding VersionType}"
                                                                           Foreground="White" />
                                                        </Border>
                                                        <Border
                                                                materialDesign:ShadowAssist.ShadowDepth="Depth0"
                                                                CornerRadius="3" HorizontalAlignment="Left"
                                                                Margin="5,0,0,0" Padding="5,2.5"
                                                                Background="#FF00AEFF">
                                                            <TextBlock Text="{Binding Time}"
                                                                           Foreground="White" />
                                                        </Border>
                                                    </StackPanel>
                                                </StackPanel>
                                                <Button
                                                        materialDesign:ShadowAssist.ShadowDepth="Depth0"
                                                        Style="{DynamicResource MaterialDesignFloatingActionButton}"
                                                        Command="{Binding DownloadLiteLoaderCommand}"
                                                        Background="{DynamicResource SystemBlue1}"
                                                        BorderBrush="{DynamicResource SystemBlue1}"
                                                        Foreground="{DynamicResource SystemGray6}" Width="30"
                                                        Height="30"
                                                        Margin="500,0,0,0" VerticalAlignment="Stretch">
                                                    <materialDesign:PackIcon Kind="Download" Width="18" Height="18" />
                                                </Button>
                                            </Grid>
                                            <materialDesign:Snackbar IsActive="{Binding InstallSnackBarActive}"
                                                                         VerticalAlignment="Center" MinWidth="0"
                                                                         Width="510" Margin="0,5,0,0">
                                                <materialDesign:SnackbarMessage>
                                                    <StackPanel Orientation="Horizontal">
                                                        <ProgressBar
                                                                Style="{StaticResource MaterialDesignCircularProgressBar}"
                                                                Background="Transparent" IsIndeterminate="True"
                                                                Foreground="{DynamicResource SystemYellow2}" />
                                                        <TextBlock Text="{Binding InstallMessage}"
                                                                       HorizontalAlignment="Center"
                                                                       VerticalAlignment="Center" />
                                                    </StackPanel>
                                                </materialDesign:SnackbarMessage>
                                            </materialDesign:Snackbar>
                                        </StackPanel>
                                    </DataTemplate>
                                </ListView.ItemTemplate>
                            </ListView>
                            <StackPanel
                                Visibility="{Binding LoadingLiteLoaderPanelVisibility, Converter={StaticResource BooleanToVisibilityConverter}}"
                                VerticalAlignment="Center" HorizontalAlignment="Center">
                                <ProgressBar
                                    Style="{StaticResource MaterialDesignCircularProgressBar}"
                                    Value="80"
                                    IsIndeterminate="True" Width="50" Height="50"
                                    Foreground="{DynamicResource SystemGreen1}" />
                                <TextBlock Foreground="{DynamicResource SystemGray6}" HorizontalAlignment="Center"
                                           Padding="5"
                                           FontSize="16" Text="{DynamicResource GetOnlineGameLoadingText}" />
                                <Button Command="{Binding GetLiteLoaderBuildsCommand}"
                                        Visibility="{Binding GetLiteLoaderAgainButtonVisibility}"
                                        Background="{DynamicResource SystemRed1}" Content="{DynamicResource Retry}"
                                        BorderBrush="{DynamicResource SystemRed1}" />
                            </StackPanel>
                        </Grid>
                    </TabItem>
                    <TabItem Background="{x:Null}" BorderBrush="{x:Null}">
                        <TabItem.Header>
                            <StackPanel Width="65" Background="#02000000">
                                <materialDesign:PackIcon Kind="Script" Width="25" Height="25"
                                                         HorizontalAlignment="Center"
                                                         Foreground="{DynamicResource SystemOrange1}" />
                                <TextBlock Foreground="{DynamicResource SystemGray6}" Text="Fabric"
                                           HorizontalAlignment="Center"
                                           Padding="5" />
                            </StackPanel>
                        </TabItem.Header>
                        <Grid Width="500" Height="350">
                            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                <TextBlock Foreground="{DynamicResource SystemYellow1}" HorizontalAlignment="Center"
                                           VerticalAlignment="Center">
                                    <materialDesign:PackIcon Kind="CommentAlert" HorizontalAlignment="Center"
                                                             VerticalAlignment="Center" Width="80" Height="80" />
                                </TextBlock>
                                <TextBlock HorizontalAlignment="Center" VerticalAlignment="Center"
                                           Foreground="{DynamicResource SystemGray6}"
                                           FontSize="24" Margin="0,40,0,0" Text="{DynamicResource StayTuned}" />
                            </StackPanel>
                        </Grid>
                    </TabItem>
                </TabControl>
            </Grid>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" VerticalAlignment="Bottom"
                        Margin="0,0,20,20">
                <Button Background="{DynamicResource SystemGreen1}" BorderBrush="{DynamicResource SystemGreen1}"
                        Content="{DynamicResource ReadMojangEULA}"
                        Command="{Binding OpenEulaPage}" materialDesign:ShadowAssist.ShadowDepth="Depth0" />
                <Button Style="{DynamicResource MaterialDesignFloatingActionButton}" Width="45" Height="45"
                        Background="{DynamicResource SystemBlue2}" BorderBrush="{DynamicResource SystemBlue2}"
                        Margin="15,0,0,0"
                        materialDesign:ShadowAssist.ShadowDepth="Depth0"
                        Command="{Binding CloseGameDownloadGridCommand}">
                    <Button.Triggers>
                        <EventTrigger RoutedEvent="Button.Click">
                            <BeginStoryboard Storyboard="{StaticResource GridBlurDrop}" />
                            <BeginStoryboard Storyboard="{StaticResource GameDownloadGridOpacityDrop}" />
                        </EventTrigger>
                    </Button.Triggers>
                    <materialDesign:PackIcon Kind="Close" Width="30" Height="30" />
                </Button>
            </StackPanel>
            <Grid HorizontalAlignment="Center" VerticalAlignment="Center"
                  Visibility="Hidden">
                <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                        <materialDesign:PackIcon Kind="AlertOutline" Foreground="{DynamicResource SystemYellow1}"
                                                 Width="60" Height="60"
                                                 HorizontalAlignment="Center" VerticalAlignment="Center" />
                        <TextBlock Foreground="{DynamicResource SystemGray6}" Text="{DynamicResource GetVersionFailed}"
                                   HorizontalAlignment="Center"
                                   VerticalAlignment="Center" FontSize="25" />
                        <Button Style="{DynamicResource MaterialDesignRaisedButton}"
                                Background="{DynamicResource SystemRed1}" Margin="0,20,0,0" Height="Auto"
                                BorderBrush="{DynamicResource SystemRed1}">
                            <Grid>
                                <StackPanel Orientation="Horizontal">
                                    <materialDesign:PackIcon Kind="Refresh" Width="25" Height="25"
                                                             Foreground="{DynamicResource SystemGray6}"
                                                             HorizontalAlignment="Center"
                                                             VerticalAlignment="Center" />
                                    <TextBlock Foreground="{DynamicResource SystemGray6}"
                                               Text="{DynamicResource Retry}" FontSize="15"
                                               HorizontalAlignment="Center" VerticalAlignment="Center" />
                                </StackPanel>
                            </Grid>
                        </Button>
                    </StackPanel>
                </Grid>
            </Grid>
        </Grid>
        <materialDesign:Snackbar x:Name="FileChooseBar"
                                 HorizontalAlignment="Stretch"
                                 IsActive="False" Background="#FF00E676" Foreground="Black"
                                 HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="20,5">
            <materialDesign:SnackbarMessage VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch"
                                            Height="140">
                <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Height="111" Margin="50,-40">
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"
                                VerticalAlignment="Center" Margin="0,10,0,10">
                        <materialDesign:PackIcon Kind="Information" HorizontalAlignment="Center"
                                                 VerticalAlignment="Center" Width="20" Height="20" />
                        <TextBlock HorizontalAlignment="Center"
                                   VerticalAlignment="Center"
                                   Text="{DynamicResource ChooseFileType}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <Button Click="MoveGameFile" Background="#7FFFFFFF" BorderBrush="{x:Null}"
                                Foreground="#DD000000" Margin="5,0" Tag="0">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="Texture" VerticalAlignment="Center"
                                                         HorizontalAlignment="Center" />
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Margin="-3,0,0,0"
                                           Padding="5,0"
                                           Text="{DynamicResource Texture}" />
                            </StackPanel>
                        </Button>
                        <Button Click="MoveGameFile" Background="#7FFFFFFF" BorderBrush="{x:Null}"
                                Foreground="#DD000000" Margin="5,0" Tag="3">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="FileImage" />
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0"
                                           Text="{DynamicResource ScreenShot}" />
                            </StackPanel>
                        </Button>
                        <Button Click="MoveGameFile" Background="#7FFFFFFF" BorderBrush="{x:Null}"
                                Foreground="#DD000000" Margin="5,0" Tag="2">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="BoxShadow" />
                                <TextBlock Padding="5,0" Text="{DynamicResource Shader}" />
                            </StackPanel>
                        </Button>
                        <Button Click="MoveGameFile" Background="#7FFFFFFF" BorderBrush="{x:Null}"
                                Foreground="#DD000000"
                                Margin="5,0" Tag="1">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="CubeUnfolded" VerticalAlignment="Center"
                                                         HorizontalAlignment="Center" />
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0"
                                           Text="{DynamicResource Mod}" />
                            </StackPanel>
                        </Button>
                        <Button Click="MoveGameFile" Background="#7FFFFFFF" BorderBrush="{x:Null}"
                                Foreground="#DD000000"
                                Margin="5,0" Tag="4">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="Map" VerticalAlignment="Center"
                                                         HorizontalAlignment="Center" />
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0"
                                           Text="{DynamicResource Map}" />
                            </StackPanel>
                        </Button>
                        <Button Click="MoveGameFile" Background="#7FFFFFFF" BorderBrush="{x:Null}"
                                Foreground="#DD000000" IsEnabled="False"
                                Margin="5,0" Tag="5">
                            <StackPanel Orientation="Horizontal">
                                <materialDesign:PackIcon Kind="ZipBox" VerticalAlignment="Center"
                                                         HorizontalAlignment="Center" />
                                <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center" Padding="5,0"
                                           Text="{DynamicResource ModPack}" />
                            </StackPanel>
                        </Button>
                    </StackPanel>
                    <Button Background="#FFFF5555" BorderBrush="{x:Null}" Foreground="#DDFFFFFF" Margin="0,10,0,0"
                            HorizontalAlignment="Center" VerticalAlignment="Center" Content="{DynamicResource Cancel}"
                            Height="Auto"
                            Command="{Binding CloseDragGridCommand}" />
                </StackPanel>
            </materialDesign:SnackbarMessage>
        </materialDesign:Snackbar>
    </Grid>
</Page>